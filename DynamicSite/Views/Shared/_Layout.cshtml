    *@


</head>
<body>
    @if (!string.IsNullOrEmpty(SessionRequest.config.BodyScript))
    {
        @Html.Raw(HttpUtility.HtmlDecode(SessionRequest.config.BodyScript));
    }

    <div class="parallax-mirror" style="visibility: hidden; z-index: -100; position: fixed; top: 0px; left: 0px; overflow: hidden; height: 736.2px; width: 1519.2px; transform: translate3d(0px, 630.313px, 0px);">
        <img class="parallax-slider" src="~/index_files/home-jobs.42785058.jpg" style="position: absolute; height: 686px; width: 1922px; max-width: none; transform: translate3d(-201px, -504.25px, 0px);">
    </div>
    <div class="preloader" style="display: none;">
        <img src="~/img/logo.svg" alt="">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>

    <div class="page-wrapper">


        @await Component.InvokeAsync("MenuView", new { type = "h" })
        @RenderBody()
        @await Component.InvokeAsync("MenuView", new { type = "f" })

    </div>



    <div class="scroll-to-top" data-target="html" style="display: block;"><i class="fas fa-chevron-up"></i></div>


    <script src="./index_files/bootstrap.min.js" type="text/javascript"></script>
    <script src="./index_files/main.3504185b.js" type="text/javascript"></script>
    <script src="./index_files/main.52155535.js" type="text/javascript"></script>


    @if (!string.IsNullOrEmpty(SessionRequest.config.FooterStyle))
    {
        @Html.Raw(HttpUtility.HtmlDecode(SessionRequest.config.FooterStyle));
    }


    @if (!string.IsNullOrEmpty(SessionRequest.config.FooterScript))
    {
        @Html.Raw(HttpUtility.HtmlDecode(SessionRequest.config.FooterScript));
    }




    @RenderSection("Scripts", required: false)
</body>
</html>
