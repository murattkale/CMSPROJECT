@{
    var _page = new ContentPage();

    if (_page != null)
    {
        _page = ViewBag.page as ContentPage;
    }
}
<!DOCTYPE html>
<html lang="tr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@SessionRequest.config.Title</title>

    <meta name="description" content="@SessionRequest.config.MetaDescription" />
    <meta name="title" content="@SessionRequest.config.Title" />
    <meta name="keywords" content="@SessionRequest.config.MetaKeywords" />

    <link rel="stylesheet" href="./index_files/bootstrap.min.css">
    <link rel="stylesheet" href="./index_files/all.css">
    <link rel="stylesheet" href="./index_files/main.57f4c744.css">

    <script src="./index_files/jquery.min.js" type="text/javascript"></script>
    <script src="~/loadingoverlay.min.js"></script>
    <script src="~/bootbox.min.js"></script>
    <script src="~/general.js?v=@SessionRequest.config.version"></script>

    <script src="./index_files/home.914fb9ec.js" type="text/javascript"></script>


    <script src="~/jquery.img2bg.min.js"></script>

    @if (!string.IsNullOrEmpty(SessionRequest.config.HeadStyle))
    {
        @Html.Raw(HttpUtility.HtmlDecode(SessionRequest.config.HeadStyle));
    }


    @if (!string.IsNullOrEmpty(SessionRequest.config.HeadScript))
    {
        @Html.Raw(HttpUtility.HtmlDecode(SessionRequest.config.HeadScript));
    }

    @*
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3012.5409120312206!2d29.082147315412783!3d40.969634979304665!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14cac7aed7af657b%3A0xb243df8cafec86bc!2zRXJlbmvDtnksIMWeZW1zZXR0aW4gR8O8bmFsdGF5IENkLiBObzoxMzUsIDM0NzM4IEthZMSxa8O2eS_EsHN0YW5idWw!5e0!3m2!1str!2str!4v1584319986069!5m2!1str!2str" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>

